<form method="post" action="/student">
  <label for="first-name-field">First Name:</label>
  <input
  type="text"
  name="first_name"
  id="first-name-field"
  />

  <label for="last-name-field">Last Name:</label>
  <input
  type="text"
  name="last_name"
  id="last-name-field"
  />

  <label for="age-field">Age:</label>
  <input
  type="number"
  name="age"
  id="age-field"
  />

  <label for="house-selection">Select a house:</label>
  <select id="house" name="house_id">
    <% for house in @house %>
     <option value="<%=house.id%>"> <%= house.name %></option>
    <%end%>
  </select>

  <input type="submit" value="send">
</form>
